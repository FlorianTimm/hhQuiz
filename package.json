    "@types/proj4": "^2.5.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "jqueryui": "^1.11.1",
    "nodemon": "^2.0.7",
    "ol": "^6.5.0",
    "proj4": "^2.7.0",
