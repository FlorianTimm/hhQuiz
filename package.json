  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "watch-ts-server": "tsc -w -p src/server/tsconfig.json",
    "watch-ts-client": "parcel watch --out-dir dist/client/ --hmr-hostname localhost --public-url . src/client/*.htm",
    "watch-node": "nodemon dist/server/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript Client, TypeScript Server, Node\" -c \"yello.bold, magenta.bold, cyan.bold\" \"yarn run watch-ts-client\" \"yarn run watch-ts-server\" \"yarn run watch-node\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
